{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
        "@/*": [
          "./src/*",
          "./dist/*",
          ""
        ]
      }
    },
    "exclude": [
      "node_modules",
      "dist"
    ]
  }